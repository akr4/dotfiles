[core]
	excludesfile = /Users/akira/.gitignore
	autocrlf = false
[alias]
	d = diff
	dd = diff -b
	st = status
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(blue)<%an>%Creset' --abbrev-commit --date=relative --date=local
	hist = log --pretty=format:'%C(red)%h%C(reset) %C(green)%ad%C(reset) | %s%C(yellow)%d%C(reset) %C(blue)[%an]%C(reset)' --graph --date=short
[color]
	ui = auto
[format]

