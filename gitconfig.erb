[user]
  name = <%= Config::NAME %>
  email = <%= Config::EMAIL %>
[core]
  excludesfile = <%= ENV['HOME'] %>/.gitignore
	autocrlf = false
[alias]
	st = status
	lg = log --pretty=format:'%C(red)%h%C(reset) %C(green)%ad%C(reset) | %s%C(yellow)%d%C(reset) %C(blue)[%an]%C(reset)' --graph --date=short
[color]
	ui = auto

